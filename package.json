{
    "name": "code-reminder",
    "displayName": "Code Reminder",
    "repository": "https://github.com/giovannyreyeso/code-reminder",
    "description": "Easy way to reminder little pieces code!",
    "version": "0.1.4",
    "publisher": "giovannyreyeso",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:code-reminder.execute"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "code-reminder.execute",
                "category": "Code reminder",
                "title": "Paste reminder code"
            }
        ],
        "keybindings": [
            {
                "command": "code-reminder.execute",
                "key": "ctrl+alt+v",
                "mac": "ctrl+v"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}